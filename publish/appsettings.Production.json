      "ProductsMinutes": 60,
      "LocationsMinutes": 60
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "SeedDatabase": false,
  "AllowedHosts": "*"
