{
  "ConnectionStrings": {
    "DefaultConnection": "Server=your-production-server;Database=your-production-db;User=your-user;Password=your-password;Port=3306;"
  },
  "Encryption": {
    "Key": "ProductionEncryptionKey2024!@#$%^&*()",
    "IV": "1234567890123456"
  },
  "Jwt": {
